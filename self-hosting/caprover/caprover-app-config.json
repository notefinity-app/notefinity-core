{
  "appName": "notefinity-core",
  "description": "Notefinity Core - Open Source Knowledge Management Backend",
  "dockerImageName": "",
  "deployedVersion": 1,
  "captainDefinitionRelativeFilePath": "./captain-definition",
  "notExposeAsWebApp": false,
  "forceSsl": true,
  "websocketSupport": false,
  "nodeId": "",
  "instanceCount": 1,
  "preDeployFunction": "",
  "serviceUpdateOverride": "",
  "customNginxConfig": "",
  "networks": [],
  "volumes": [],
  "ports": [],
  "appPushWebhook": {
    "repoInfo": {
      "user": "",
      "repo": "",
      "branch": "main"
    },
    "pushedAt": ""
  },
  "customDomain": [],
  "hasDefaultSubDomainSsl": false,
  "redirectDomain": "",
  "envVars": [
    {
      "key": "PORT",
      "value": "3000"
    },
    {
      "key": "NODE_ENV",
      "value": "production"
    },
    {
      "key": "COUCHDB_URL",
      "value": "http://your-couchdb-instance:5984"
    },
    {
      "key": "COUCHDB_USERNAME",
      "value": "admin"
    },
    {
      "key": "COUCHDB_PASSWORD",
      "value": "your-secure-password"
    },
    {
      "key": "JWT_SECRET",
      "value": "your-jwt-secret-key-minimum-32-characters"
    },
    {
      "key": "ALLOWED_ORIGINS",
      "value": "https://yourdomain.com"
    },
    {
      "key": "RATE_LIMIT_WINDOW_MS",
      "value": "900000"
    },
    {
      "key": "RATE_LIMIT_MAX_REQUESTS",
      "value": "100"
    },
    {
      "key": "LOG_LEVEL",
      "value": "info"
    }
  ]
}
